# MoltApp Environment Variables
# Copy to .env and fill in your values

# === Database ===
DATABASE_URL=postgresql://user:pass@localhost:5432/moltapp

# === Moltbook Identity ===
MOLTBOOK_APP_KEY=your_moltbook_app_key

# === Turnkey Wallet Infrastructure ===
TURNKEY_ORGANIZATION_ID=your_turnkey_org_id
TURNKEY_API_PUBLIC_KEY=your_turnkey_public_key
TURNKEY_API_PRIVATE_KEY=your_turnkey_private_key

# === Solana ===
HELIUS_API_KEY=your_helius_api_key
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY

# === Jupiter DEX ===
JUPITER_ULTRA_API_KEY=your_jupiter_api_key

# === Security ===
ADMIN_PASSWORD=your_admin_password
WEBHOOK_SECRET=your_webhook_secret

# === AI Providers (at least one required for trading) ===
GOOGLE_API_KEY=your_gemini_api_key
OPENAI_API_KEY=your_openai_api_key
# ANTHROPIC_API_KEY=your_anthropic_api_key
# XAI_API_KEY=your_xai_api_key

# === Agent Wallets (optional â€” auto-generated by onboarding CLI) ===
# ONBOARD_WALLET_PUBLIC=
# ONBOARD_WALLET_SECRET=
# GEMINI_WALLET_PUBLIC=
# ANTHROPIC_WALLET_PUBLIC=
# OPENAI_WALLET_PUBLIC=
# GROK_WALLET_PUBLIC=

# === Colosseum Hackathon ===
COLOSSEUM_API_KEY=your_colosseum_api_key

# === Server Configuration ===
PORT=3000
NODE_ENV=development

# === Demo Mode ===
# Set to "true" to enable demo mode - simulates trading without real blockchain transactions
# Perfect for hackathon judges to try MoltApp without real wallets or funds
# Agents get 100 SOL + 10,000 USDC starting balance in demo mode
DEMO_MODE=false
